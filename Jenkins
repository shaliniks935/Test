pipeline {
	agent any
		stages {
			stage ('BUILD'){
				steps {
					echo "This is Build stage and completed sucessfully"
					  }
				   }  
				
			stage ('TEST'){
				steps {
					echo "This is Test stage and completed sucessfully"
					  }
				   }  	
				
			stage ('DEPLOY'){
				steps {
					echo "This is Deploy stage and completed sucessfully"
					  }
				   }
				 }
			    }
